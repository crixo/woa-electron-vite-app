{
  "name": "woa",
  "type": "module",
  "version": "1.0.7",
  "description": "Web Osteopathic Application",
  "main": "./out/main/index.mjs",
  "author": "Crixo",
  "copyright": "Â© 2025 Crixo All rights reserved.",
  "homepage": "https://github.com/crixo/",
  "repository": "https://github.com/crixo/woa-electron-vite-app",
  "scripts": {
    "format": "prettier --write .",
    "lint": "eslint --cache .",
    "start": "electron-vite preview",
    "dev": "electron-vite dev",
    "build": "electron-vite build",
    "postinstall": "electron-builder install-app-deps",
    "build:unpack": "npm run build && electron-builder --dir",
    "build:win": "npm run build && electron-builder --win",
    "build:linux": "npm run build && electron-builder --linux",
    "rebuild-sqlite3": "electron-rebuild -f -w better-sqlite3",
    "skip_prebuild:mac": "node scripts/updateVersion.js",
    "build:mac": "npm run rebuild-sqlite3 && npm run build && electron-builder --mac",
    "build:mac-x64": "npm run rebuild-sqlite3-x64 && npm run build && electron-builder --mac --x64",
    "prebuild:mac-x64": "node scripts/updateVersion.js",
    "build:mac-universal": "npm run build && electron-builder --mac --universal",
    "rebuild-sqlite3-x64": "electron-rebuild -f -w --arch=x64 better-sqlite3",
    "prerelease:mac": "node scripts/updateVersion.js",
    "release:mac": "npm run rebuild-sqlite3 && npm run build && electron-builder --mac --publish 'onTagOrDraft'",
    "test:main": "electron-mocha tests/main --renderer=false"
  },
  "dependencies": {
    "@electron-toolkit/preload": "^3.0.1",
    "@electron-toolkit/utils": "^4.0.0",
    "@huggingface/inference": "^4.0.6",
    "better-sqlite3": "^11.10.0",
    "electron-log": "^5.4.0",
    "electron-updater": "^6.6.2",
    "js-yaml": "^4.1.0",
    "openai": "^5.3.0",
    "undici": "^7.10.0"
  },
  "devDependencies": {
    "@electron-toolkit/eslint-config": "^2.0.0",
    "@electron-toolkit/eslint-config-prettier": "^3.0.0",
    "@fortawesome/fontawesome-free": "^6.7.2",
    "@tailwindcss/vite": "^4.1.6",
    "@vitejs/plugin-react": "^4.3.4",
    "@vitest/ui": "^3.2.4",
    "@welldone-software/why-did-you-render": "^10.0.1",
    "chai": "^5.2.0",
    "dompurify": "^3.2.6",
    "electron": "^35.5.1",
    "electron-builder": "^25.1.8",
    "electron-devtools-installer": "^4.0.0",
    "electron-mocha": "^13.1.0",
    "electron-rebuild": "^3.2.9",
    "electron-vite": "^3.1.0",
    "eslint": "^9.24.0",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "jsdom": "^26.1.0",
    "luxon": "^3.6.1",
    "marked": "^15.0.12",
    "mocha": "^11.7.0",
    "prettier": "^3.5.3",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-error-boundary": "^6.0.0",
    "react-router-dom": "^7.6.0",
    "react-toastify": "^11.0.5",
    "tailwindcss": "^4.1.6",
    "vite": "^6.2.6",
    "vite-plugin-static-copy": "^3.0.0",
    "vitest": "^3.2.4"
  }
}
